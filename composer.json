{
    "name": "avonis/php-respimg",
    "description": "A responsive image workflow for optimizing and resizing your images",
    "version": "2.0.0",

    "homepage": "https://github.com/avonis/php-respimg/",
    "license": "MIT",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/avonis/php-respimg/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "gumlet/php-image-resize": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "avonis\\": "src/",
            "": "src/"
        }
    }
}
